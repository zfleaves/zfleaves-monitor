#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/study/zfleaves-monitor/node_modules/.pnpm/encode-bundle@1.4.1_@swc+core@1.11.6_ts-node@9.1.1_typescript@5.8.2__typescript@5.8.2/node_modules/encode-bundle/dist/node_modules:/mnt/e/study/zfleaves-monitor/node_modules/.pnpm/encode-bundle@1.4.1_@swc+core@1.11.6_ts-node@9.1.1_typescript@5.8.2__typescript@5.8.2/node_modules/encode-bundle/node_modules:/mnt/e/study/zfleaves-monitor/node_modules/.pnpm/encode-bundle@1.4.1_@swc+core@1.11.6_ts-node@9.1.1_typescript@5.8.2__typescript@5.8.2/node_modules:/mnt/e/study/zfleaves-monitor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/study/zfleaves-monitor/node_modules/.pnpm/encode-bundle@1.4.1_@swc+core@1.11.6_ts-node@9.1.1_typescript@5.8.2__typescript@5.8.2/node_modules/encode-bundle/dist/node_modules:/mnt/e/study/zfleaves-monitor/node_modules/.pnpm/encode-bundle@1.4.1_@swc+core@1.11.6_ts-node@9.1.1_typescript@5.8.2__typescript@5.8.2/node_modules/encode-bundle/node_modules:/mnt/e/study/zfleaves-monitor/node_modules/.pnpm/encode-bundle@1.4.1_@swc+core@1.11.6_ts-node@9.1.1_typescript@5.8.2__typescript@5.8.2/node_modules:/mnt/e/study/zfleaves-monitor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli-default.js" "$@"
else
  exec node  "$basedir/../../dist/cli-default.js" "$@"
fi
